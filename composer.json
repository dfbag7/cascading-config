{
    "name": "dimbo/cascading-config",
    "description": "Cascading configuration support for Laravel 8",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Bagdanov",
            "email": "dimbo_job@mail.ru"
        }
    ],
    "keywords": ["Laravel", "CascadingConfig"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Dimbo\\CascadingConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dimbo\\CascadingConfig\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dimbo\\CascadingConfig\\CascadingConfigServiceProvider"
            ]
        }
    }
}
